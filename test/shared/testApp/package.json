{
  "name": "wolkenkit-test",
  "version": "1.1.1",
  "wolkenkit": {
    "application": "test",
    "runtime": {
      "version": "1.2.0"
    },
    "environments": {
      "default": {
        "api": {
          "address": {
            "host": "local.wolkenkit.io",
            "port": 9000
          },
          "allowAccessFrom": "*"
        },
        "identityProvider": {
          "name": "test.wolkenkit.io",
          "certificate": "/server/keys/io.wolkenkit.test"
        }
      }
    }
  },
  "dependencies": {
    "wolkenkit-command-tools": "1.0.0"
  },
  "devDependencies": {
    "roboter": "0.15.4",
    "roboter-server": "0.15.4"
  },
  "license": "AGPL-3.0"
}
